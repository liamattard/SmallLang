<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<Program name="example.smalllang">
  <FunctionDeclaration name="Square" type="FLOAT">
    <Param name="x" type="FLOAT" />
    <Block>
      <ReturnNode>
        <MultiplicativeOp operator="TIMES">
          <Identifier>x</Identifier>
          <Identifier>x</Identifier>
        </MultiplicativeOp>
      </ReturnNode>
    </Block>
  </FunctionDeclaration>
  <FunctionDeclaration name="XGreaterThanY" type="AUTO">
    <Param name="x" type="FLOAT" />
    <Param name="y" type="FLOAT" />
    <Block>
      <VariableDeclaration Type="BOOL">
        <Identifier>ans</Identifier>
        <BoolConst>true</BoolConst>
      </VariableDeclaration>
      <IfStatement>
        <RelationalOp operator="GREATERTHAN">
          <Identifier>y</Identifier>
          <Identifier>x</Identifier>
        </RelationalOp>
        <Block Validity="True">
          <AssignmentStatement>
            <Identifier>ans</Identifier>
            <BoolConst>false</BoolConst>
          </AssignmentStatement>
        </Block>
      </IfStatement>
      <ReturnNode>
        <Identifier>ans</Identifier>
      </ReturnNode>
    </Block>
  </FunctionDeclaration>
  <FunctionDeclaration name="AverageOfThree" type="FLOAT">
    <Param name="x" type="FLOAT" />
    <Param name="y" type="FLOAT" />
    <Param name="z" type="FLOAT" />
    <Block>
      <VariableDeclaration Type="FLOAT">
        <Identifier>total</Identifier>
        <AdditiveOp Operator="PLUS">
          <Identifier>x</Identifier>
          <AdditiveOp Operator="PLUS">
            <Identifier>y</Identifier>
            <Identifier>z</Identifier>
          </AdditiveOp>
        </AdditiveOp>
      </VariableDeclaration>
      <ReturnNode>
        <MultiplicativeOp operator="DIVISION">
          <Identifier>total</Identifier>
          <IntegerConst>3</IntegerConst>
        </MultiplicativeOp>
      </ReturnNode>
    </Block>
  </FunctionDeclaration>
  <VariableDeclaration Type="FLOAT">
    <Identifier>x</Identifier>
    <FloatConst>2.4</FloatConst>
  </VariableDeclaration>
  <VariableDeclaration Type="AUTO">
    <Identifier>y</Identifier>
    <FunctionCall name="Square">
      <FloatConst>2.5</FloatConst>
    </FunctionCall>
  </VariableDeclaration>
  <PrintStatement>
    <Identifier>y</Identifier>
  </PrintStatement>
  <PrintStatement>
    <FunctionCall name="XGreaterThanY">
      <Identifier>x</Identifier>
      <FloatConst>2.3</FloatConst>
    </FunctionCall>
  </PrintStatement>
  <PrintStatement>
    <FunctionCall name="XGreaterThanY">
      <FunctionCall name="Square">
        <FloatConst>1.5</FloatConst>
      </FunctionCall>
      <Identifier>y</Identifier>
    </FunctionCall>
  </PrintStatement>
</Program>